.weui-toast {
  width: 250rpx;
  height: 250rpx;
}

.weui-toast__content {
  font-size: 30rpx;
}

.nopermissions {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10000;
}

.nopermissions_content {
  width: 586rpx;
  background: white;
  border-radius: 20rpx;
  margin: 0 auto;
  margin-top: 400rpx;

}

.nopermissions_top {
  width: 100%;
  line-height: 74rpx;
  box-sizing: border-box;
  padding: 40rpx 60rpx;
  font-size: 34rpx;
  color: #666;
  text-align: center;
  border-bottom: 1rpx solid #dedede;
}

.nopermissions_bottom {
  width: 100%;
  height: 104rpx;
  font-size: 32rpx;
  color: #666;
  text-align: center;
  line-height: 104rpx;
}
.fadeIn{
  width: 100%;
  height: 100vh;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, .5);
}