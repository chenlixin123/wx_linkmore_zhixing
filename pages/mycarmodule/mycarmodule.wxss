  justify-content: space-between;
  align-items: center;
  margin-top: 30rpx;
  margin-left: 30rpx;
  /* border: 1px solid; */
}

.box_top_line_left,
.box_top_line_right {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.box_top_line_left_text {
  font-size: 30rpx;
  height: 31rpx;
  line-height: 31rpx;
  /* border: 1px solid; */
}

.box_top_line_left_number {
  font-size: 30rpx;
  height: 31rpx;
  line-height: 31rpx;
  color: #ff8234;
  margin-left: 11rpx;
}

.box_top_line_right {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.box_top_img {
  width: 28rpx;
  height: 28rpx;
  margin-right: 6rpx;
}

.box_top_line_right_text {
  font-size: 24rpx;
  height: 24rpx;
  line-height: 24rpx;
  color: #999;
}

.box_br {
  width: 630rpx;
  border-top: 1px solid #f0efef;
