.box_top {
  width: 689rpx;
  height: 219rpx;
  background: #FFFFFF;
  border-radius: 26rpx;
  margin-top: 23rpx;
  margin-left: 30rpx;
  border: 1px solid #fff;
}

.box_top_line {
  width: 631rpx;
  height: 31rpx;
